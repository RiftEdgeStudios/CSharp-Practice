                            int rangerAbility = rnd.Next(16, 22);
                            int rangerUltimate = rnd.Next(23, 40);
                            playerTwoTurn = false;
                            Console.WriteLine("");
                            Console.WriteLine(playerTwoName + ": " + rangerHealth + "HP " + rangerMana + " Mana");
                            Console.WriteLine("(1) Accuracy and Precision (Basic): Scope in your opponent and shoot a bullet straight into their gut.");
