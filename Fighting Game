                                    int brawlerAbility = rnd.Next(10, 15);
                                    int brawlerUltimate = rnd.Next(15, 25);
                                    int cpuDamageNormal = rnd.Next(12, 22);
                                    int cpuDamageUltimate = rnd.Next(23, 40);
