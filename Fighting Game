                                Console.WriteLine("");
                                Console.WriteLine("Right through the chest! " + playerTwoName + " snipes " + playerTwoName + ", tearing a hole into their chest and shooting out " + rangerAbility + " HP!");
                                Console.WriteLine(playerTwoName + " now has " + rangerMana + " Mana left.");
                                playerOneTurn = true;
                            }
                            else if (rangerMana < 6 && rangerAttackChoice == "two" || rangerAttackChoice == "2")
                            {
                                Console.WriteLine("");
                                Console.WriteLine("Not enough Mana!");
                                playerOneTurn = true;
                            }
                            else
                            {
                                Console.WriteLine("");
                                Console.WriteLine("Please say either 1 or 2!");
                                playerTwoTurn = true;
                            }
                        }
