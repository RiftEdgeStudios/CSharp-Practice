                                    if (brawlerAttackChoice == "1" || brawlerAttackChoice == "one" && ultimateRangerCount < 6)
                                    {
                                        rangerHealth = rangerHealth - brawlerBasic;
                                        brawlerHealth = brawlerHealth - cpuDamageNormal;
                                        ultimateRangerCount = ultimateRangerCount + 1;
                                        ultimateBrawlerCount = ultimateBrawlerCount + 1;
                                        Console.WriteLine("");
                                        Console.WriteLine("Nice Hit! " + playerOneName + " rocks and socks the CPU into oblivion, taking away " + brawlerBasic + " HP!");
                                        Console.WriteLine("");
